    <dependencies>
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-test</artifactId>
            <scope>test</scope>
        </dependency>
        <dependency>
            <groupId>com.yuanxiaoyuan</groupId>
            <artifactId>spring-cloud-alibaba-common-base</artifactId>
            <version>1.0.0-SNAPSHOT</version>
        </dependency>
    </dependencies>

    <build>
        <plugins>
            <plugin>
                <groupId>org.springframework.boot</groupId>
                <artifactId>spring-boot-maven-plugin</artifactId>
                <configuration>
                    <mainClass>com.yuanxiaoyuan.springcloudalibabanacos.SpringCloudAlibabaNacosApplication</mainClass>
                </configuration>
            </plugin>
        </plugins>
    </build>

</project>
